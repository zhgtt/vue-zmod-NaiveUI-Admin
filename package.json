    "dayjs": "^1.11.19",
    "es-toolkit": "^1.44.0",
    "naive-ui": "^2.43.2",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^25.2.3",
